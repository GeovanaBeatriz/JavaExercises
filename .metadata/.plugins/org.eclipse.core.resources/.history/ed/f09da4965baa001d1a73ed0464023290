package curso_programacao;


import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int A, B, produto;
	    
	    A = sc.nextInt();
	    
	    B = sc.nextInt();
	    
	    produto = A * B;

	    
	    System.out.printf("PROD = "+ produto);

		sc.close();
	}

}
